{
    "editor.fontWeight": "bold",
    "editor.fontFamily": "'fira code medium'",
    "editor.fontSize": 13,
    "editor.fontLigatures": true,
    "gitlens.views.repositories.location": "gitlens",
    "gitlens.views.fileHistory.location": "gitlens",
    "gitlens.views.lineHistory.location": "gitlens",
    "gitlens.views.compare.location": "gitlens",
    "gitlens.views.search.location": "gitlens",
    "[json]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "explorer.confirmDragAndDrop": false,
    "explorer.confirmDelete": false,
    "todo-tree.tree.showScanModeButton": false,
    "editor.wordWrap": "on",
    "editor.acceptSuggestionOnEnter": "off",
    "files.associations": {
        "*.rmd": "markdown"
    },
    "markdown.preview.fontFamily": "-apple-system, BlinkMacSystemFont,'Iosevka', 'fira code'",
    "markdown-preview-enhanced.previewTheme": "monokai.css",
    "plantuml.exportConcurrency": 6,
    "plantuml.exportFormat": "latex",
    "rainbow_csv.comment_prefix": "#",
    "rainbow_csv.csv_lint_detect_trailing_spaces": true,
    "editor.cursorBlinking": "expand",
    "editor.cursorStyle": "underline",
    "editor.minimap.maxColumn": 100,
    "editor.minimap.scale": 2,
    "files.defaultLanguage": "${activeEditorLanguage}",
    "files.eol": "\n",
    "files.insertFinalNewline": true,
    "workbench.iconTheme": "Monokai Pro Icons",
    "[yaml]": {
        "editor.defaultFormatter": "redhat.vscode-yaml"
    },
    "[dockerfile]": {
        "editor.defaultFormatter": "ms-azuretools.vscode-docker"
    },
    "cSpell.language": "en,pt",
    "[markdown]": {
        "editor.defaultFormatter": "yzhang.markdown-all-in-one"
    },
    "[typescript]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "[html]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "markdown-preview-enhanced.codeBlockTheme": "monokai.css",
    "gitlens.gitCommands.closeOnFocusOut": true,
    "[javascript]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "redhat.telemetry.enabled": false,
    "[jsonc]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "editor.codeActionsOnSave": null,
    "editor.bracketPairColorization.enabled": true,
    "cSpell.userWords": [
        "configurability",
        "Flink",
        "Kubernetes",
        "microservices",
        "proxying",
        "roadmap",
        "Sensae"
    ],
    "[typescriptreact]": {
        "editor.defaultFormatter": "vscode.typescript-language-features"
    },
    "[dockercompose]": {
        "editor.defaultFormatter": "ms-azuretools.vscode-docker"
    },
    "security.workspace.trust.untrustedFiles": "open",
    "terminal.integrated.enableMultiLinePasteWarning": false,
    "latex-workshop.bibtex-fields.sort.enabled": true,
    "plantuml.jar": "/usr/share/java/plantuml/plantuml.jar",
    "plantuml.diagramsRoot": "assets/diagrams",
    "plantuml.exportOutDir": "assets/diagrams",
    "plantuml.exportSubFolder": false,
    "latex-workshop.latex.tools": [
        {
            "name": "latexmk",
            "command": "latexmk",
            "args": [
                "-lualatex",
                "--shell-escape",
                "-synctex=1",
                "-interaction=nonstopmode",
                "-file-line-error",
                "-outdir=%OUTDIR%",
                "%DOC%"
            ]
        },
        {
            "name": "pdflatex",
            "command": "pdflatex",
            "args": [
                "--shell-escape",
                "-synctex=1",
                "-interaction=nonstopmode",
                "-file-line-error",
                "%DOC%"
            ]
        },
        {
            "name": "bibtex",
            "command": "bibtex",
            "args": [
                "%DOCFILE%"
            ]
        }
    ],
    "latex-workshop.latex.magic.args": [
        "-synctex=1",
        "-interaction=nonstopmode",
        "-file-line-error",
        "%DOC%",
        "-shell-escape",
        "--shell-escape"
    ],
    "latex-workshop.latexindent.args": [
        "-c",
        "%DIR%/",
        "%TMPFILE%",
        "-y=defaultIndent: '%INDENT%'",
        "--shell-escape",
        "-shell-escape"
    ],
    "cSpell.minWordLength": 3,
    "cSpell.showAutocompleteSuggestions": true,
    "cSpell.checkLimit": 1000,
    "cSpell.enabledLanguageIds": [
        "asciidoc",
        "c",
        "cpp",
        "csharp",
        "css",
        "elixir",
        "erlang",
        "git-commit",
        "go",
        "graphql",
        "handlebars",
        "haskell",
        "html",
        "jade",
        "java",
        "javascript",
        "javascriptreact",
        "json",
        "jsonc",
        "jupyter",
        "latex",
        "less",
        "markdown",
        "php",
        "plaintext",
        "python",
        "pug",
        "restructuredtext",
        "rust",
        "scala",
        "scss",
        "scminput",
        "swift",
        "text",
        "typescript",
        "typescriptreact",
        "vue",
        "yaml",
        "yml",
        "tex"
    ],
    "cSpell.enableFiletypes": [
        "latex"
    ],
    "latex-workshop.view.pdf.viewer": "browser",
    "latex-workshop.intellisense.biblatexJSON.replace": {
    
    },
    "editor.unicodeHighlight.nonBasicASCII": false,
    "javascript.updateImportsOnFileMove.enabled": "never",
    "workbench.colorTheme": "Gruvbox Material Dark",
}
